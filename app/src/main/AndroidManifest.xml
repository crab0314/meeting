<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crab.doit.meetingonline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name_china"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.LoginActivity"></activity>
        <activity android:name=".activity.SignUpActivity"></activity>
        <activity android:name=".activity.HomeActivity"></activity>
        <activity android:name=".activity.AddPartyActivity"></activity>
        <activity android:name=".activity.SearchFriendActivity"></activity>
        <activity android:name=".activity.OnMeetingActivity"></activity>
    </application>

</manifest>